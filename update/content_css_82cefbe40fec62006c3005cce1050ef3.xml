&#13;
/* Toggle Styles */&#13;
&#13;
#wrapper {&#13;
    padding-left: 0;&#13;
    -webkit-transition: all 0.5s ease;&#13;
    -moz-transition: all 0.5s ease;&#13;
    -o-transition: all 0.5s ease;&#13;
    transition: all 0.5s ease;&#13;
}&#13;
&#13;
#wrapper.toggled {&#13;
    padding-left: 250px;&#13;
}&#13;
&#13;
#sidebar-wrapper {&#13;
   padding: 5px;&#13;
    z-index: 1000;&#13;
    position: fixed;&#13;
    right: 250px;&#13;
    width: 0;&#13;
    height: 100%;&#13;
    margin-right: -250px;&#13;
    overflow-y: auto;&#13;
    background: #f5f5f5;&#13;
