    top: 0;&#13;
    width: 250px;&#13;
    margin: 0;&#13;
    padding: 0;&#13;
    list-style: none;&#13;
}&#13;
&#13;
.sidebar-nav li {&#13;
    text-indent: 20px;&#13;
    line-height: 40px;&#13;
}&#13;
&#13;
.sidebar-nav li a {&#13;
    display: block;&#13;
    text-decoration: none;&#13;
    color: #5f5f5f;&#13;
}&#13;
&#13;
.sidebar-nav li a:hover {&#13;
    text-decoration: none;&#13;
    color: #fff;&#13;
    background: rgba(255,255,255,0.2);&#13;
}&#13;
&#13;
.sidebar-nav li a:active,&#13;
.sidebar-nav li a:focus {&#13;
    text-decoration: none;&#13;
}&#13;
&#13;
.sidebar-nav &gt; .sidebar-brand {&#13;
    height: 65px;&#13;
    font-size: 18px;&#13;
    line-height: 60px;&#13;
}&#13;
&#13;
.sidebar-nav &gt; .sidebar-brand a {&#13;
    color: #999999;&#13;
}&#13;
&#13;
.sidebar-nav &gt; .sidebar-brand a:hover {&#13;
    color: #fff;&#13;
    background: none;&#13;
}&#13;
&#13;
@media(min-width:768px) {&#13;
    #wrapper {&#13;
        padding-right: 250px;&#13;
    }&#13;
&#13;
    #wrapper.toggled {&#13;
        padding-left: 0;&#13;
    }&#13;
&#13;
    #sidebar-wrapper {&#13;
        width: 250px;&#13;
    }&#13;
&#13;
    #wrapper.toggled #sidebar-wrapper {&#13;
