							<label class="checkbox-inline">
								<input type="checkbox"  ng-model="mc.model.u_high_email" /> eMail
							</label>
							<label class="checkbox-inline">
								<input type="checkbox"  ng-model="mc.model.u_high_sms" /> SMS
							</label>						
						</div>
					</div>				
				</div>
				<div class="col-sm-12">
					<div class="form-group form-group-sm">
						<label class="col-sm-4 control-label">Problem Notifications</label>
						<div class="col-sm-8">
							<label class="checkbox-inline">
								<input type="checkbox" ng-model="mc.model.u_product_email" /> eMail
							</label>
							<label class="checkbox-inline">
								<input type="checkbox"  ng-model="mc.model.u_product_sms" /> SMS
							</label>						
						</div>
					</div>				
				</div>
				<div class="col-sm-12">
					<div class="form-group form-group-sm">
						<label class="col-sm-4 control-label">Change notifications</label>
